meta {
  name: Get Status - Player 1 Waiting
  type: http
  seq: 2
}

get {
  url: {{baseUrl}}/api/matchmaking/status
  body: none
  auth: bearer
}

auth:bearer {
  token: {{whiteToken}}
}

tests {
  test("Status is 200", function() {
    expect(res.status).to.equal(200);
  });

  test("Status is WAITING", function() {
    expect(res.body.status).to.equal("WAITING");
  });
}
