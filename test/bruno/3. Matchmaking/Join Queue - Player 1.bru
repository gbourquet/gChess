meta {
  name: Join Queue - Player 1
  type: http
  seq: 1
}

post {
  url: {{baseUrl}}/api/matchmaking/queue
  body: none
  auth: bearer
}

auth:bearer {
  token: {{whiteToken}}
}

tests {
  test("Status is 200", function() {
    expect(res.status).to.equal(200);
  });

  test("Status is WAITING", function() {
    expect(res.body.status).to.equal("WAITING");
  });

  test("Queue position is 1", function() {
    expect(res.body.queuePosition).to.equal(1);
  });

  test("No game ID yet", function() {
    expect(res.body.gameId).to.be.null;
  });
}
